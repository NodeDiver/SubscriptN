{
  "name": "nwc-periodic-payments",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8080",
    "build": "next build",
    "start": "next start",
    "lint": "tsc -noEmit && next lint && yarn format:check",
    "format:fix": "yarn prettier --write . --check '!.db'",
    "format:check": "yarn prettier --check . --check '!.db'",
    "inngest:local": "inngest-cli dev",
    "docker:start": "docker-compose up",
    "docker:stop": "docker-compose stop",
    "docker:remove": "docker-compose down",
    "db:migrate:local": "dotenv -e .env.local -- prisma migrate dev",
    "db:migrate:local:createonly": "dotenv -e .env.local -- prisma migrate dev --create-only",
    "db:connect:local": "psql postgres://user:password@localhost:5432/boostagram-viewer",
    "db:studio:local": "dotenv -e .env.local -- prisma studio",
    "db:reset:local": "dotenv -e .env.local -- prisma migrate reset",
    "deploy": "fly deploy --remote-only --build-secret DATABASE_URL=$DATABASE_URL",
    "db:migrate:deploy": "prisma migrate deploy",
    "prepare": "husky install"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "4.12.0",
    "@types/node": "18.15.11",
    "@types/react": "18.0.31",
    "@types/react-dom": "18.0.11",
    "alby-js-sdk": "^1.4.2",
    "alby-tools": "^2.1.0",
    "encoding": "^0.1.13",
    "eslint": "8.37.0",
    "eslint-config-next": "13.2.4",
    "http-status-codes": "^2.2.0",
    "inngest": "^1.5.3",
    "next": "13.2.4",
    "next-auth": "^4.20.1",
    "nostr-tools": "^1.8.1",
    "prisma": "^4.12.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.8",
    "typescript": "5.0.2",
    "websocket-polyfill": "^0.0.3"
  },
  "devDependencies": {
    "dotenv-cli": "^7.1.0",
    "husky": "^8.0.0",
    "inngest-cli": "^0.11.0",
    "prettier": "^2.8.7"
  }
}
